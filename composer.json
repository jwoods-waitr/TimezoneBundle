{
    "name": "lunetics/timezone-bundle",
    "description": "A Bundle for detecting the timezone",
    "keywords": ["timezone", "bundle", "time zone"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lunetics",
            "homepage": "http://www.lunetics.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.2",
        "symfony/yaml": "*",
        "symfony/validator": "*"
    },
    "require-dev": {
        "ext-intl": "*"
    },
    "suggest": {
          "ext-intl": "Always a good idea :)",
          "ext-geoip": "*"
    },
    "autoload": {
        "psr-0": { "Lunetics\\TimezoneBundle": "" }
    },
    "target-dir": "Lunetics/TimezoneBundle"
}
